{
	"name": "reduced",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"dev": "nodemon server/index.ts",
		"dev:js": "nodemon dist/index.js --watch server",
		"build": "npm run build:client && npm run build:server",
		"build:client": "cross-env NODE_ENV=production nuxt-ts generate",
		"build:server": "cross-env NODE_ENV=production tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./server -o ./dist",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"start:client": "nuxt-ts start"
	},
	"dependencies": {
		"@nuxt/typescript-runtime": "^2.0.0",
		"@nuxtjs/auth-next": "^5.0.0-1608461079.ae1e5db",
		"@nuxtjs/axios": "^5.12.4",
		"bcryptjs": "^2.4.3",
		"core-js": "^3.6.5",
		"cross-env": "^7.0.3",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-useragent": "^1.0.15",
		"joi": "^17.3.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.11.4",
		"nuxt": "^2.14.6",
		"nuxt-vuex-localstorage": "^1.2.7",
		"particles.js": "^2.0.0",
		"serve-static": "^1.14.1"
	},
	"devDependencies": {
		"@nuxt/types": "^2.14.12",
		"@nuxt/typescript-build": "^2.0.3",
		"@nuxtjs/vuetify": "^1.11.2",
		"@types/bcrypt": "^3.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.9",
		"@types/express-useragent": "^1.0.0",
		"@types/http-errors": "^1.8.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mongoose": "~5.10.2",
		"@types/node": "^14.14.14",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-prettier": "^3.1.4",
		"nodemon": "^2.0.6",
		"prettier": "^2.1.2",
		"ts-node": "^9.0.0",
		"tsconfig-paths": "^3.9.0",
		"tscpaths": "0.0.9",
		"typescript": "^4.1.2"
	}
}
